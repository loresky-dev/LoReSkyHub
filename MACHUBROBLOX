TitleLabel.Size = UDim2.new(1, 0, 0, 50)
TitleLabel.Position = UDim2.new(0, 0, 0, 0)
TitleLabel.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
TitleLabel.BorderSizePixel = 0
TitleLabel.Text = "MAC HUB"
TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TitleLabel.TextScaled = true
TitleLabel.Font = Enum.Font.SourceSansBold
TitleLabel.Parent = MainFrame

-- Add UI corner to MainFrame (optional for rounded corners)
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 10)
UICorner.Parent = MainFrame

-- Add UI corner to TitleLabel (optional for rounded corners)
local TitleCorner = Instance.new("UICorner")
TitleCorner.CornerRadius = UDim.new(0, 10)
TitleCorner.Parent = TitleLabel

-- Create the Scripts Frame (pop-up menu)
local ScriptsFrame = Instance.new("Frame")
ScriptsFrame.Position = UDim2.new(0.5, -150, 0.5, -100) -- Center the pop-up within MainFrame
ScriptsFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
ScriptsFrame.BorderSizePixel = 0
ScriptsFrame.Visible = false -- Hidden initially
ScriptsFrame.Parent = MainFrame

-- Create a button for Infinite Yield
local InfiniteYieldButton = Instance.new("TextButton")
InfiniteYieldButton.Size = UDim2.new(0, 300, 0, 50) -- Button size to fit inside ScriptsFrame
InfiniteYieldButton.Position = UDim2.new(0.6, 70, 0.5, -40) -- Centered within ScriptsFrame
InfiniteYieldButton.BorderSizePixel = 0
InfiniteYieldButton.Text = "Infinite Yield"
InfiniteYieldButton.TextColor3 = Color3.fromRGB(255, 255, 255)
InfiniteYieldButton.TextScaled = true
InfiniteYieldButton.Font = Enum.Font.SourceSansBold
InfiniteYieldButton.Parent = ScriptsFrame

-- Add rounded corners to InfiniteYieldButton
local InfiniteYieldButtonCorner = Instance.new("UICorner")
InfiniteYieldButtonCorner.CornerRadius = UDim.new(0, 10)
InfiniteYieldButtonCorner.Parent = InfiniteYieldButton

-- Define the loadInfiniteYield function
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end

-- Connect the button to the script function
InfiniteYieldButton.MouseButton1Click:Connect(loadInfiniteYield)

-- Show ScriptsFrame when Admin button is clicked
AdminButton.MouseButton1Click:Connect(function()
    ScriptsFrame.Visible = not ScriptsFrame.Visible -- Toggle visibility of ScriptsFrame
end)

-- Play sound when GUI opens
local Sound = Instance.new("Sound")
Sound.SoundId = "rbxassetid://3109186986"
Sound.Volume = 1
Sound.PlayOnRemove = true -- Ensures the sound plays immediately
Sound.Parent = ScreenGui
Sound:Destroy() -- Triggers the sound

-- Make the GUI draggable
local UIS = game:GetService("UserInputService")
local Dragging
local DragInput
local DragStart
local StartPos

local function Update(input)
    local Delta = input.Position - DragStart
    MainFrame.Position = UDim2.new(StartPos.X.Scale, StartPos.X.Offset + Delta.X, StartPos.Y.Scale, StartPos.Y.Offset + Delta.Y)
end

MainFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        Dragging = true
        DragStart = input.Position
        StartPos = MainFrame.Position

        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                Dragging = false
            end
        end)
    end
end)

MainFrame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        DragInput = input
    end
end)

UIS.InputChanged:Connect(function(input)
    if input == DragInput and Dragging then
        Update(input)
    end
end)
