wait(0.2)
local function callback(text)
    if text == "Yes" then
	print("User said yes")
    elseif text == "No" then
	print("User said no")
    end
end
 
local bindableFunction= Instance.new("BindableFunction")
 
game.StarterGui:SetCore("SendNotification", {
    Title = "Loaded!";
    Text = "Have fun!";
    Duration = "2";
    callbakc = bindableFunction;
    Button1 = "OK";
 
})
 
wait(3.5)

-- Create the ScreenGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "MacHubGui"
ScreenGui.Parent = game.CoreGui

-- Create the main frame
local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 500, 0, 400)
MainFrame.Position = UDim2.new(0.5, -250, 0.5, -200) -- Center the frame
MainFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
MainFrame.BorderSizePixel = 0
MainFrame.Parent = ScreenGui

-- Create the sidebar
local Sidebar = Instance.new("Frame")
Sidebar.Size = UDim2.new(0, 150, 1, 0) -- Sidebar takes up 150px of width and full height of MainFrame
Sidebar.Position = UDim2.new(0, 0, 0, 0)
Sidebar.BackgroundColor3 = Color3.fromRGB(60, 60, 60) -- Lighter color for the sidebar
Sidebar.BorderSizePixel = 0
Sidebar.Parent = MainFrame

-- Create the Admin button
local AdminButton = Instance.new("TextButton")
AdminButton.Size = UDim2.new(1, -20, 0, 50) -- Full width of the sidebar minus padding, height of 60px
AdminButton.Position = UDim2.new(0, 10, 0, 60) -- Positioned 10 pixels down from the top of the sidebar
AdminButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70) -- Slightly darker button background
AdminButton.BorderSizePixel = 0
AdminButton.Text = "Admin"
AdminButton.TextColor3 = Color3.fromRGB(255, 255, 255)
AdminButton.TextScaled = true
AdminButton.Font = Enum.Font.SourceSansBold
AdminButton.Parent = Sidebar

-- Add rounded corners to the Admin button
local ButtonCorner = Instance.new("UICorner")
ButtonCorner.CornerRadius = UDim.new(0, 10) -- Rounded corners with a radius of 10
ButtonCorner.Parent = AdminButton

-- Create the text label
